{
    "name": "verge/laravel-openapi-generator",
    "description": "Automatically generate OpenAPI 3.1 specifications from Laravel routes, requests, and responses",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Verge Development Team",
            "email": "dev@verge.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "symfony/yaml": "^6.0|^7.0",
        "nikic/php-parser": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Verge\\OpenAPIGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Verge\\OpenAPIGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Verge\\OpenAPIGenerator\\OpenAPIGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
